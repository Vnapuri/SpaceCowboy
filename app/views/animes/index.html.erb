
<div class="anime_list">
    <% @animes.each do |anime| %>
        <div class="index-tile">
            <img class="index-img" src="<%=anime.poster_image%>" alt="" style="width:210px;height:180px;object-fit: cover;">
            <span><%= link_to anime.title, anime_path(anime) %></span>
        </div>
    <%end%>
</div>